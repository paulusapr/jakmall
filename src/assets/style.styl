$background = #fffae6
$theme = #ff8a00
$themeoff = #ffe4b8
$text = #ffffff
$link = #000000
$subtext = #2d2a40
$checkbox = #1bd97b
$border = #eeeeee
$pickbg = rgba(27, 217, 123, 0.1)
$inputborder = #cccccc

a
  text-decoration: none
  color: $link
  cursor: pointer
#app
  width: 100%
  height: 100%
  display: flex
  justify-content: center
  align-items: center
  padding: 5% 5%
.container
  width: 100%
  height: 100%
  display: flex
  border-radius: 4px
  box-shadow: 2px 10px 20px 0 rgba(255, 138, 0, 0.1)
  background-color: #ffffff
  position: relative